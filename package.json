{
  "name": "@prosync/ui-component-kit",
  "version": "0.1.0",
  "type": "module",
  "description": "Reusable Vue 3 component library with PrimeVue and Tailwind CSS for Prosync applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prosync-2026/ui-component-kit.git"
  },
  "bugs": {
    "url": "https://github.com/prosync-2026/ui-component-kit/issues"
  },
  "homepage": "https://github.com/prosync-2026/ui-component-kit#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "build:all": "npm run build && npm run build:types",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "primevue": "^3.50.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@storybook/addon-a11y": "^10.0.6",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/addon-onboarding": "^10.0.6",
    "@storybook/addon-vitest": "^10.0.6",
    "@storybook/vue3-vite": "^10.0.6",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "primevue": "^3.50.0",
    "storybook": "^10.0.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.1.0",
    "vitest": "^4.0.8",
    "playwright": "^1.56.1",
    "@vitest/browser-playwright": "^4.0.8",
    "@vitest/coverage-v8": "^4.0.8"
  },
  "keywords": [
    "vue",
    "vue3",
    "component-library",
    "ui-components-kit",
    "prosync",
    "primevue",
    "tailwindcss",
    "typescript"
  ],
  "author": "Ryedonna Epin",
  "license": ""
}
